# inputs
# INPUT sw1 FTS14EM 0xfefee20e
INPUT sw2 FTS14EM 0x0000002
INPUT sw3 FTS14EM 0x0000003

# GPIO
INPUT gpio1 GPI 19
OUTPUT gpio2 GPO 20

# outputs
OUTPUT dim1 FUD14 0xEEEEEE00
OUTPUT dim2 FUD14 0xEEEEEE01
OUTPUT dim3 FUD14 0xEEEEEE02

# timer
#TIMER t1 5s
#IF t1 TIMEOUT
#DO gpio2 SET
#DO t1 RESET
#DO t1 SET


#IF sw1 SHORT_PRESS
#DO l1 TOGGLE
#DO t1 SET
#IF t1 TIMEOUT
#DO l1 UNSET

# rules
#IF sw1 SHORT_PRESS
#DO dim1 TOGGLE
#IF sw1 DIM
#DO dim1 DIM

#IF sw2 SHORT_PRESS
#DO dim2 TOGGLE
